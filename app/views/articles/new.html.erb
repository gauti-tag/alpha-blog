<h1>Create a new file</h1>

<%= form_with scope: :article, url: articles_path, local: true do |f| %>

  <p>
     <%= f.label :title %> <br />
     <%= f.text_field :title %>
  </p>

  <p>
    <%= f.label :desciption %> <br />
    <%= f.text_area :desciption %>
  </p>

  <p>
    <%= f.submit %>
  </p>

<% end %>